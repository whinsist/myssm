<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.com.cloudstar.rightcloud.system.dao.act.ProcessMapper">
    <resultMap id="BaseResultMap" type="cn.com.cloudstar.rightcloud.system.pojo.dto.process.Process">
        <result column="id" property="id" jdbcType="BIGINT" />
        <result column="process_code" property="processCode" />
        <result column="process_name" property="processName" />
        <result column="business_code" property="businessCode" />
        <result column="business_name" property="businessName" />
        <result column="org_sid" property="orgSid" jdbcType="BIGINT" />
        <result column="description" property="description" />
        <result column="created_by" property="createdBy" />
        <result column="created_dt" property="createdDt" />
        <result column="updated_by" property="updatedBy" />
        <result column="updated_dt" property="updatedDt" />
        <result column="status" property="status" />
    </resultMap>
    <sql id="Base_Column_List">
        id,process_code,process_name,business_code,business_name,org_sid,
        description,created_by,created_dt,updated_by,updated_dt,`status`
    </sql>
    <update id="updateByPrimaryKeySelective" parameterType="cn.com.cloudstar.rightcloud.system.pojo.dto.process.Process">
        update process
        <set>
            <if test="processCode != null">
                process_code = #{processCode},
            </if>
            <if test="processName != null">
                process_name = #{processName},
            </if>
            <if test="businessCode != null">
                business_code = #{businessCode},
            </if>
            <if test="businessName != null">
                business_name = #{businessName},
            </if>
            <if test="orgSid != null">
                org_sid = #{orgSid},
            </if>
            <if test="description != null">
                description = #{description},
            </if>
            <if test="createdBy != null">
                created_by = #{createdBy},
            </if>
            <if test="createdDt != null">
                created_dt = #{createdDt},
            </if>
            <if test="updatedBy != null">
                updated_by = #{updatedBy},
            </if>
            <if test="updatedDt != null">
                updated_dt = #{updatedDt},
            </if>
            <if test="status != null">
                status = #{status},
            </if>
        </set>
        where id=#{id}
    </update>
    <delete id="deleteByPrimaryKey" parameterType="java.lang.Long">
        delete from process where id = #{id}
    </delete>
    <delete id="clear">
        delete from process where 1=1
    </delete>
    <select id="selectAll" resultMap="BaseResultMap">
      select
      <include refid="Base_Column_List"/>
      from process where status != 104
    </select>
    <select id="selectByPrimaryKey" resultMap="BaseResultMap" parameterType="java.lang.Long">
        select
        <include refid="Base_Column_List"/>
        from process where id=#{id}
    </select>
    <select id="selectByProcessCode" resultMap="BaseResultMap" parameterType="java.lang.String">
        select
        <include refid="Base_Column_List"/>
        from process where process_code=#{code} and status != 104 limit 1
    </select>
    <select id="selectByProcessName" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List"/>
        from process where process_name=#{name} and org_sid=${orgSid} and status != 104 limit 1
    </select>
    <select id="selectByBusinessCode" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List"/>
        from process where business_code=#{code} and org_sid=${orgSid} and status != 104 limit 1
    </select>
    <select id="selectByBusinessCodeWithDeleted" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List"/>
        from process where business_code=#{code} and org_sid=${orgSid} limit 1
    </select>
    <select id="selectByParams"
            parameterType="cn.com.cloudstar.rightcloud.system.pojo.dto.process.Process"
            resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List"/>
        from process
        <if test="_parameter != null">
            <trim prefix="where" prefixOverrides="and|or">
                status != 104
                <if test="id != null">
                    and id = #{id}
                </if>
                <if test="processCode != null">
                    and process_code = #{processCode}
                </if>
                <if test="processName != null">
                    and process_name like concat('%', #{processName}, '%')
                </if>
                <if test="businessCode != null">
                    and business_code = #{businessCode}
                </if>
                <if test="orgSid != null">
                    and org_sid = #{orgSid}
                </if>
                <if test="description != null">
                    and description like concat('%', #{description}, '%')
                </if>
            </trim>
        </if>
    </select>
    <select id="countByOrgSid"
            parameterType="java.lang.Long" resultType="java.lang.Integer">
        select count(1) from process where status != 104 and org_sid = #{orgSid}
    </select>
    <insert id="insertSelective"
            parameterType="cn.com.cloudstar.rightcloud.system.pojo.dto.process.Process"
            useGeneratedKeys="true" keyProperty="id">
        insert into process
        <trim prefix="(" suffix=")" suffixOverrides=",">
            id,
            <if test="processCode != null">
                process_code,
            </if>
            <if test="processName != null">
                process_name,
            </if>
            <if test="businessCode != null">
                business_code,
            </if>
            <if test="businessName != null">
                business_name,
            </if>
            <if test="orgSid != null">
                org_sid,
            </if>
            <if test="description != null">
                description,
            </if>
            <if test="createdBy != null">
                created_by,
            </if>
            <if test="createdDt != null">
                created_dt,
            </if>
            <if test="updatedBy != null">
                updated_by,
            </if>
            <if test="updatedDt != null">
                updated_dt,
            </if>
            <if test="status != null">
                status,
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            #{id},
            <if test="processCode != null">
                #{processCode},
            </if>
            <if test="processName != null">
                #{processName},
            </if>
            <if test="businessCode != null">
                #{businessCode},
            </if>
            <if test="businessName != null">
                #{businessName},
            </if>
            <if test="orgSid != null">
                #{orgSid},
            </if>
            <if test="description != null">
                #{description},
            </if>
            <if test="createdBy != null">
                #{createdBy},
            </if>
            <if test="createdDt != null">
                #{createdDt},
            </if>
            <if test="updatedBy != null">
                #{updatedBy},
            </if>
            <if test="updatedDt != null">
                #{updatedDt},
            </if>
            <if test="status != null">
                #{status},
            </if>
        </trim>
    </insert>
</mapper>
